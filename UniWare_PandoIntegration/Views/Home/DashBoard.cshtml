<style>
    /* Color and Images for Project Containers  */

    /* Sizing for Project Containers  */
    .column-4 {
        float: left;
        width: 16%;
        padding: 10px;
        margin: 20px;
        height: 119px;
    }

    .project-tiles {
        background-color: #dc3545;
    }

    .pointer {
        cursor: pointer;
    }


    .project-text {
        text-align: center;
        font-size: 20px;
        color: #fff;
    }

    .project-tiles:hover {
        color: #dc3545;
    }

    /* Hover classes for individual project boxes */
    .project-tiles:hover {
        background-color: gray;
    }

    .dashboard_tiles {
        margin-top: 30px;
        padding: 50px;
        /* box-shadow: 1px 1px 10px 2px gray; */
    }

    .tile_list {
        color: #dc3545;
        font-weight: 700;
        box-shadow: 1px 1px 10px 2px gray;
    }

        .tile_list:hover {
            color: #fff;
            background: #dc3545
        }

        .tile_list p {
            margin: 10px !important;
        }

    .title_dashboard {
        color: #dc3545;
        font-size: 2rem;
        text-shadow: 2px 2px;
    }


    .loader {
        display: none;
        position: absolute;
        left: 50%;
        top: 50%;
        width: 120px;
        height: 120px;
        margin: -76px 0 0 -76px;
        /*background:white;*/
        border: 16px solid #f3f3f3;
        border-radius: 50%;
        border-top: 16px solid #3498db;
        -webkit-animation: spin 2s linear infinite;
        animation: spin 2s linear infinite;
        z-index: 99999;
    }



    @@keyframes spin {
        0% {
            transform: rotate(0deg);
        }

        100% {
            transform: rotate(360deg);
        }
    }

    a.logo:hover, a.notification_bell_icon:hover, a.waves-dark.pro-pic:hover {
        text-decoration: none !important;
    }

    .dataTables_length {
        display: none;
    }

</style>

